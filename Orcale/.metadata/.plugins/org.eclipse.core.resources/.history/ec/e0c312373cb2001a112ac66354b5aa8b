package com.capgemini.oracle.corrected;

public class Example81 {
	int x;
	int y;

	public void doStuff(int x, int y) {
		this.x = x;
		y = this.y;

	}

	public void display() {
		System.out.println(x+""+y+":");
	}
	
	public static void main(String[] args) {
		Example81 ex1 = new Example81();
		ex1.x=100;
		ex1.y=200;
		Example81 ex2 = new Example81();
		ex2.doStuff(ex1.x, ex1.y);
		ex1.display();
		ex2.display();
	}
}
